{
  "name": "jordan-zaatari-map",
  "scripts": {
    "appcache": "npm run appcache-manifest && npm run appcache-manifest-fixer",
    "appcache-manifest": "appcache-manifest ./deploy/*.* ./deploy/{dist,resources}/*.* ./deploy/**/**/*.* ./deploy/**/**/**/*.* ./deploy/**/**/**/**/*.* --network-star -o ./deploy/index.appcache",
    "appcache-manifest-fixer": "appcache-manifest-fixer ./deploy/index.html -m ./deploy/index.appcache -o ./deploy/legacy.html",
    "build": "webpack -p",
    "deploy": "npm run build && ./deploy-dist.sh && npm run appcache && npm run sw-precache",
    "sw-precache": "node ./node_modules/sw-precache/cli.js --root=deploy --maximumFileSizeToCacheInBytes=10485760",
    "start": "webpack-dev-server --content-base ./ --hot --inline",
    "test": "./node_modules/.bin/eslint ./src/"
  },
  "devDependencies": {
    "appcache-manifest": "^1.0.4",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "pre-commit": "^1.1.3",
    "sw-precache": "^4.2.1",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11"
  },
  "dependencies": {
    "@turf/center": "^3.5.3",
    "aphrodisiac": "^0.3.0",
    "intl": "^1.2.5",
    "intl-messageformat": "^1.3.0",
    "mapbox-gl": "^0.27.0",
    "normalize.css": "^5.0.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "redux": "^3.6.0",
    "whatwg-fetch": "^2.0.0"
  }
}
